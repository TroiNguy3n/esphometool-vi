{
  "name": "ESPHomeTool-vi",
  "version": "1.12.2",
  "slug": "esphome",
  "description": "Đây là công cụ hổ trợ biên dịch mã yaml sang mã arduino cho module esp32/esp8266.",
  "url": "https://github.com/TroiNguy3n/esphometool-vi/",
  "webui": "http://[HOST]:[PORT:6052]",
  "startup": "application",
  "arch": [
    "amd64",
    "armhf",
    "i386"
  ],
  "hassio_api": true,
  "auth_api": true,
  "hassio_role": "default",
  "homeassistant_api": false,
  "host_network": true,
  "boot": "auto",
  "auto_uart": true,
  "map": [
    "ssl",
    "config:rw"
  ],
  "options": {
    "ssl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "port": 6052
  },
  "schema": {
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
    "port": "int",
    "leave_front_door_open": "bool?",
    "esphome_version": "str?",
    "streamer_mode": "bool?",
    "relative_url": "str?",
    "status_use_ping": "bool?"
  },
  "image": "TroiNguy3n/TroiNguy3n-esphome-{arch}"
}
